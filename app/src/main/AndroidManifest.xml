<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sample.trabalho">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.AUTHENTICATE_ACCOUNTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="ConnectionBR"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Trabalho">
        <activity android:name=".HistoricoActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sample.trabalho.DasboardActivity" />
        </activity>
        <activity android:name=".CompraVendaActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sample.trabalho.DasboardActivity" />
        </activity>
        <activity android:name=".DepositarActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sample.trabalho.DasboardActivity" />
        </activity>
        <activity android:name=".CadastroActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sample.trabalho.MainActivity" />
        </activity>
        <activity android:name=".DasboardActivity"></activity>
        <activity android:name=".CotacaoActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sample.trabalho.DasboardActivity" />
        </activity>
        <activity android:name=".GraficoActivity">
            android:screenOrientation="landscape"
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sample.trabalho.DasboardActivity" />
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>